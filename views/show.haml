%html
  %head
    %link{:href => "/css/style.css", :rel => "stylesheet", :media => "screen"}

  %body
    %h2= key

    %ul#history
      - history.each do |key|
        %li
          %a{:href => "./#{key}"}= key


    #values
      - if values.is_a?(String)
        = values
      - else
        %ul
          - values.each do |value| 
            %li.value= value

    %textarea#config
      = redis.inspect.to_s
